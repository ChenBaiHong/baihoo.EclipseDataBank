<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- demo6Bean的集合属性注入==================================== -->
	<bean id="collectionBean" class="com.baihoo.spring4.demo6.CollectionBean">
		<!-- 注入List集合 -->
		<property name="list">
			<list>
				<value>去年今日此门中，</value>
				<value>人面桃花相映红。</value>
			</list>
		</property>

		<!-- 注入set集合 -->
		<property name="set">
			<set>
				<value>天子守国门，</value>
				<value>君王死社稷。</value>
			</set>
		</property>

		<!-- 注入map集合 -->
		<property name="map">
			<map>
				<entry key="李白" value="静夜思" />
				<entry key="鲁迅" value="朝花夕拾" />
			</map>
		</property>
	
		<property name="properties">
			<props>
				<prop key="username">root</prop>
				<prop key="password">root</prop>
			</props>
		</property>
	</bean>
	<!-- demo6Bean的集合属性注入==================================end -->
</beans>